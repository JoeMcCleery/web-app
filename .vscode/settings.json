{
  "files.associations": {
    "*.zmodel": "zmodel",
    "*.css": "tailwindcss"
  },
  "editor.formatOnSave": true,
  "[zmodel]": {
    "editor.defaultFormatter": "zenstack.zenstack"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.classAttributes": ["class", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ]
}
